{
    "name": "myquote-dev/xero",
    "description": "A fork of Dan Ursu's (daursu/xero) elegant Laravel 4 wrapper for the official Xero API.",
    "keywords": ["laravel","illuminate","xero"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0",
        "xero/xerooauth-php": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Daursu\\Xero\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
